###################################################################################################
### COUPLING: BoundaryCondition - External Stress #################################################
###################################################################################################

[Multiscale]
couplingName    = BoundaryCondition_Stress_ExternalPressure

[boundary_conditions]
list = 'Stress_ExternalPressure'  # External pressure

    [./Stress_ExternalPressure]
    type                = Natural
    mode                = Normal
    functionSolver      = '-s_externalPressure'

    # Additional parameters for 1D BC
    quantity            = S
    line                = first

    # Additional parameters for 0D BC
    type0D              = Voltage

    [../]
