###################################################################################################
### COUPLING: BoundaryCondition - Stress Atrium ###################################################
###################################################################################################

[Multiscale]
couplingName    = BoundaryCondition_Stress_Atrium

[boundary_conditions]
list = 'Stress_Atrium'

    [./Stress_Atrium]
    type                = Natural
    mode                = Normal
    functionFile        = FunctionsDatabase/LeftAtriumPressure.dat

    # Additional parameters for 1D BC
    quantity            = S
    line                = first

    # Additional parameters for 0D BC
    type0D              = Voltage

    [../]
