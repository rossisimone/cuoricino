###################################################################################################
### LEFT ATRIUM PRESSURE ##########################################################################
###################################################################################################
# First column:  time [s]
# Second column: pressure in left atrium (including A-wave)
#
# One loop correspond to one heart pulse (pulse / minute: 75)
###################################################################################################

function  = 'p'

variables = 't            p'
scale     = '1            -11000'

loop      = true

# Items must be sorted by the first column value!
data        =  '0.00      0.0
                0.10      0.0
                0.11      1.0
                0.60      1.0
                0.70      1.0
                0.80      0.0'
