    </projectdoc>
    <ignoreqt_xml/>
    <ignoredoxygen/>
    <ignorekdocs/>
    <ignoretocs/>
    <ignoredevhelp/>
  </kdevdoctreeview>
  <kdevdocumentation>
    <projectdoc>
      <docsystem/>
      <docurl/>
      <usermanualurl>/home/prudhomm/scratch/Devel/LifeV/lifev-head/debug-3.3/doc/api/index.html</usermanualurl>
    </projectdoc>
