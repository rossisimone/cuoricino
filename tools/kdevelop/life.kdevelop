  </general>
  <kdevautoproject>
    <general>
      <activetarget>testsuite/official/test_mesh/test_mesh</activetarget>
      <useconfiguration>debug-3.3</useconfiguration>
    </general>
    <run>
      <mainprogram/>
      <directoryradio>executable</directoryradio>
      <customdirectory>/</customdirectory>
      <programargs/>
      <terminal>false</terminal>
      <autocompile>true</autocompile>
      <envvars/>
    </run>
    <configurations>
      <optimized>
        <builddir>optimized</builddir>
        <ccompiler>GccOptions</ccompiler>
        <cxxcompiler>GppOptions</cxxcompiler>
        <f77compiler>G77Options</f77compiler>
        <cxxflags>-O2 -g0</cxxflags>
      </optimized>
      <debug>
        <configargs>--enable-debug=full</configargs>
        <builddir>debug</builddir>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevg77options</f77compiler>
        <cxxflags>-O0 -g3</cxxflags>
        <topsourcedir/>
        <cppflags/>
        <ldflags/>
        <ccompilerbinary/>
        <cxxcompilerbinary>g++-3.3</cxxcompilerbinary>
        <f77compilerbinary>g77</f77compilerbinary>
        <cflags>-O0 -g3</cflags>
        <f77flags>-O0 -g3</f77flags>
        <envvars/>
      </debug>
      <debug-3.3>
        <envvars/>
        <configargs>--enable-debug --with-mkl=/opt/intel/mkl701/
</configargs>
        <builddir>/home/prudhomm/scratch/Devel/LifeV/lifev-head/debug-3.3</builddir>
        <topsourcedir>/home/prudhomm/Devel/LifeV/lifev-head</topsourcedir>
        <cppflags>-I/home/prudhomm/Devel/LifeV/lifev-head</cppflags>
        <ldflags/>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevg77options</f77compiler>
        <ccompilerbinary>gcc-3.3</ccompilerbinary>
        <cxxcompilerbinary>g++-3.3</cxxcompilerbinary>
        <f77compilerbinary>g77-3.3</f77compilerbinary>
        <cflags>-O0 -g3</cflags>
        <cxxflags>-O0 -g3</cxxflags>
        <f77flags>-O0 -g3</f77flags>
      </debug-3.3>
      <opt-3.3>
        <envvars/>
        <configargs>--enable-opt --with-mkl=/opt/intel/mkl701/
</configargs>
        <builddir>/home/prudhomm/scratch/Devel/LifeV/lifev-head/opt-3.3</builddir>
        <topsourcedir>/home/prudhomm/Devel/LifeV/lifev-head</topsourcedir>
        <cppflags/>
        <ldflags/>
        <ccompiler>kdevgccoptions</ccompiler>
        <cxxcompiler>kdevgppoptions</cxxcompiler>
        <f77compiler>kdevg77options</f77compiler>
        <ccompilerbinary>gcc-3.3</ccompilerbinary>
        <cxxcompilerbinary>g++-3.3</cxxcompilerbinary>
        <f77compilerbinary/>
        <cflags/>
        <cxxflags/>
        <f77flags/>
      </opt-3.3>
    </configurations>
    <make>
      <envvars>
        <envvar value="1" name="WANT_AUTOCONF_2_5" />
        <envvar value="1" name="WANT_AUTOMAKE_1_6" />
      </envvars>
      <abortonerror>true</abortonerror>
      <numberofjobs>1</numberofjobs>
      <dontact>false</dontact>
      <makebin/>
      <prio>0</prio>
    </make>
  </kdevautoproject>
  <kdevdebugger>
    <general>
      <dbgshell>libtool</dbgshell>
      <programargs/>
      <gdbpath/>
      <configGdbScript/>
      <runShellScript/>
      <runGdbScript/>
      <breakonloadinglibs>true</breakonloadinglibs>
      <separatetty>false</separatetty>
      <floatingtoolbar>false</floatingtoolbar>
    </general>
    <display>
      <staticmembers>false</staticmembers>
      <demanglenames>true</demanglenames>
      <outputradix>10</outputradix>
    </display>
  </kdevdebugger>
  <kdevfilecreate>
